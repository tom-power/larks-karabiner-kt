{
  "title": "larks navigation",
  "rules": [
    {
      "description": "Page up (left_control+backspace)",
      "manipulators": [
        {
          "from": {
            "key_code": "delete_or_backspace",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "key_code": "page_up"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Page down (left_control+enter)",
      "manipulators": [
        {
          "from": {
            "key_code": "return_or_enter",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "key_code": "page_down"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Page up fn (fn+backspace)",
      "manipulators": [
        {
          "from": {
            "key_code": "delete_or_backspace",
            "modifiers": {
              "mandatory": [
                "fn"
              ]
            }
          },
          "to": [
            {
              "key_code": "page_up"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Page down fn (fn+backspace)",
      "manipulators": [
        {
          "from": {
            "key_code": "return_or_enter",
            "modifiers": {
              "mandatory": [
                "fn"
              ]
            }
          },
          "to": [
            {
              "key_code": "page_down"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Page up control (left_control+up)",
      "manipulators": [
        {
          "from": {
            "key_code": "up_arrow",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "key_code": "page_up"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Page down control (left_control+down)",
      "manipulators": [
        {
          "from": {
            "key_code": "down_arrow",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "key_code": "page_down"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
