{
  "title": "larks mac to pc",
  "rules": [
    {
      "description": "@ (left_shift + ')",
      "manipulators": [{
        "from": {
          "key_code": "quote",
          "modifiers": {
            "mandatory": [
              "left_shift"
            ]
          }
        },
        "to": [{
          "key_code": "2",
          "modifiers": [
            "left_shift"
          ]
        }],
        "type": "basic"        
      }]
    },
    {
      "description": "\" (left_shift + 2)",
      "manipulators": [{
        "from": {
          "key_code": "2",
          "modifiers": {
            "mandatory": [
              "left_shift"
            ]
          }
        },
        "to": [{
          "key_code": "quote",
          "modifiers": [
            "left_shift"
          ]
        }],
        "type": "basic"
      }]
    },
    {
      "description": "\" (right_shift + 2)",
      "manipulators": [{
        "from": {
          "key_code": "2",
          "modifiers": {
            "mandatory": [
              "right_shift"
            ]
          }
        },
        "to": [{
          "key_code": "quote",
          "modifiers": [
            "left_shift"
          ]
        }],
        "type": "basic"
      }]
    },
    {
      "description": "# (right_shift + 3)",
      "manipulators": [{
        "from": {
          "key_code": "3",
          "modifiers": {
            "mandatory": [
              "right_shift"
            ]
          }
        },
        "to": [{
          "key_code": "3",
          "modifiers": [
            "left_option"
          ]
        }],
        "type": "basic"
      }]
    },
    {
      "description": "~/ (right_shift + ยง)",
      "manipulators": [{
        "from": {
          "key_code": "non_us_backslash",
          "modifiers": {
            "mandatory": [
              "right_shift"
            ]
          }
        },
        "to": [{
          "key_code": "grave_accent_and_tilde",
          "modifiers": [
            "left_shift"
          ]
        }, {
          "key_code": "slash"
        }],
        "type": "basic"
      }]
    }
  ]
}
